{
  "name": "claude-projects-sdk",
  "type": "module",
  "version": "0.1.2",
  "description": "Unofficial Node.js SDK for Claude Code.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./schemas": {
      "types": "./dist/common/schemas.d.ts",
      "import": "./dist/common/schemas.mjs",
      "require": "./dist/common/schemas.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pkgroll",
    "dev": "tsx watch src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write src test",
    "format:check": "prettier --check src",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "claude",
    "claude-code",
    "anthropic",
    "ai",
    "projects",
    "sdk"
  ],
  "author": "Carlos <carloseliesersantos@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carlelieser/claude-projects-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/carlelieser/claude-projects-sdk/issues"
  },
  "homepage": "https://github.com/carlelieser/claude-projects-sdk#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "fast-glob": "^3.3.3",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.0.9",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "jiti": "^2.6.1",
    "pkgroll": "^2.21.5",
    "prettier": "^3.8.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.0",
    "vitest": "^4.0.17"
  }
}
